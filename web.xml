<?xml version="1.0" encoding="ISO-8859-1"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0"
  metadata-complete="true">

<jsp-config>
  <jsp-property-group>
    <url-pattern>
    *.jsp
    </url-pattern>
    <page-encoding>
    UTF-8
    </page-encoding>
  </jsp-property-group>
</jsp-config>
<servlet>



<servlet-name>REDIRECT_TO_REGRISTRITATION</servlet-name>
<servlet-class>REDIRECT_TO_REGRISTRITATION</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>REDIRECT_TO_REGRISTRITATION</servlet-name>
<url-pattern>/reg_form</url-pattern>
</servlet-mapping>



<servlet>
<servlet-name>DB_CONNECTOR_TEST</servlet-name>
<servlet-class>DB_CONNECTOR_TEST</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>DB_CONNECTOR_TEST</servlet-name>
<url-pattern>/db_test</url-pattern>
</servlet-mapping>



<servlet>
<servlet-name>FETCH_REGRISTER_DATA</servlet-name>
<servlet-class>FETCH_REGRISTER_DATA</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>FETCH_REGRISTER_DATA</servlet-name>
<url-pattern>/fetch_reg_data</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>LOGIN_CHECK</servlet-name>
<servlet-class>LOGIN_CHECK</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>LOGIN_CHECK</servlet-name>
<url-pattern>/login_check</url-pattern>
</servlet-mapping>



<servlet>
<servlet-name>END_SESSION</servlet-name>
<servlet-class>end_session.END_SESSION</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>END_SESSION</servlet-name>
<url-pattern>/end_session</url-pattern>
</servlet-mapping>
 

 
<servlet>
<servlet-name>SESSION_CREATOR</servlet-name>
<servlet-class>session_handler.SESSION_CREATOR</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>SESSION_CREATOR</servlet-name>
<url-pattern>/session_handler</url-pattern>
</servlet-mapping>


<servlet>
<servlet-name>PROP_INTERMEDIATE</servlet-name>
<servlet-class>PROP_INTERMEDIATE_PACK.PROP_INTERMEDIATE</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>PROP_INTERMEDIATE</servlet-name>
<url-pattern>/PROP_INTERMEDIATE</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>ADD_TO_CART1</servlet-name>
<servlet-class>ADD_TO_CART_PACK.ADD_TO_CART_SERVLET</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>ADD_TO_CART1</servlet-name>
<url-pattern>/ADD_TO_CART</url-pattern>
</servlet-mapping>



<servlet>
<servlet-name>REMOVE_FROM_CART1</servlet-name>
<servlet-class>remove_from_cart.REMOVE_FROM_CART</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>REMOVE_FROM_CART1</servlet-name>
<url-pattern>/remove</url-pattern>
</servlet-mapping>


<servlet>
<servlet-name>LOGOUTUSER</servlet-name>
<servlet-class>LOGOUT_USER.LOGOUT</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>LOGOUTUSER</servlet-name>
<url-pattern>/logoutt</url-pattern>
</servlet-mapping>


<welcome-file-list>
<welcome-file>index.jsp</welcome-file>
</welcome-file-list>


</web-app>
